@page "/"

    @if (Subscriptions != null)
    {
        @foreach (var g in Subscriptions)
        {
            @g.RouteData.RouteId
            @if (g.IsActive)
            {
                <b style="color:red">!</b>
            }
        }
    }


<SubscriptionComponent Subscriptions="@Subscriptions" OnSelectionChange="@RouteChecked"></SubscriptionComponent>

<div class="container">
    <h3>Map</h3>
    <div id="osm-map"></div>
    <div class="map-parent">
    </div>
</div>